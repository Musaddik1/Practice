<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<display-name>login</display-name>
  	<description></description>
  	<servlet-class>com.Servlet.login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterationServlet</servlet-name>
  	<display-name>RegisterationServlet</display-name>
  	<description></description>
  	<servlet-class>com.Servlet.RegisterationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ForgetServlet</servlet-name>
  	<display-name>ForgetServlet</display-name>
  	<description></description>
  	<servlet-class>com.Servlet.ForgetServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SetPassword</servlet-name>
  	<display-name>SetPassword</display-name>
  	<description></description>
  	<servlet-class>com.Servlet.SetPassword</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterationServlet</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ForgetServlet</servlet-name>
  	<url-pattern>/forget</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SetPassword</servlet-name>
  	<url-pattern>/set</url-pattern>
  </servlet-mapping>
 
</web-app>
